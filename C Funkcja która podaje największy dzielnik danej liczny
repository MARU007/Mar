#include <stdio.h>
#include <stdlib.h>
int liczba (int n){
    int i, c,s=1;
  //  printf("%d", n);
   for (i=1; i<n; i++) {
      if(n%i == 0) {
            if (s<i)
                s=i;

        }

        }

       /*  for (i = (n-1); i>0; i--) {
        if(n%i == 0) {
            return i;
        }
    }*/
    //nie ma dzielnika
return s;
}
int main()
{   int n, c;
    printf("Podaj liczbÃª n:\n");
